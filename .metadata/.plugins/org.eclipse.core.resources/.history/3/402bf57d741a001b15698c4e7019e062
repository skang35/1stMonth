package Algorithm;

class Apple {
	Apple() {
		System.out.println("Parents");
	}

}

class Banana extends Apple{
	Banana() {
		System.out.println("Kids");
	}

}

public class SangSok {
	public static void main(String[] args) {
//		Apple apple = new Apple();
		Banana b1 = new Banana();
	}
}

