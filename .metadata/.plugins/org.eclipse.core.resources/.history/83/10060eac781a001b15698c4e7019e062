package Algorithm;

class Apple {
	Apple() {
		
	}
	void func1() {
		System.out.println("부모 1");
	}
	void func3() {
		System.out.println("부모 3");
	}

}
class Orange extends Apple{
	Orange(){
		
	}
	void func2() {
		System.out.println("자식 2");
	}
	void func4() {
		System.out.println("자식 3");
	}
}

